{data}) => {\n                console.log(data.message)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        },\n\n        deleteTask(id){\n            axios({\n                method: \"DELETE\",\n                url: `http://localhost:3000/tasks/${id}`,\n                headers: {'access_token': localStorage.access_token}\n            })\n            .then(({data}) => {\n                console.log(data.message)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        }\n\n    }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"064a15b6bf8bcc2ef584f4ca614c03e5","cacheData":{"env":{}}}